<phpunit
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="https://schema.phpunit.de/8.0/phpunit.xsd"
        backupGlobals="true"
        bootstrap="tests/bootstrap.php"
        cacheResult="false"
        cacheTokens="false"
        verbose="true"
        colors="true"
>
    <testsuites>
        <testsuite name="client">
            <directory>tests</directory>
        </testsuite>
    </testsuites>
</phpunit>
